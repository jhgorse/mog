widgets.app/Contents/PkgInfo: widgets$(EXEEXT) $(top_srcdir)/src/osx/carbon/Info.plist.in $(top_srcdir)/src/osx/carbon/wxmac.icns
	mkdir -p widgets.app/Contents
	mkdir -p widgets.app/Contents/MacOS
	mkdir -p widgets.app/Contents/Resources
	
	
	sed -e "s/IDENTIFIER/`echo $(srcdir) | sed -e 's,\.\./,,g' | sed -e 's,/,.,g'`/" \
	-e "s/EXECUTABLE/widgets/" \
	-e "s/VERSION/$(WX_VERSION)/" \
	$(top_srcdir)/src/osx/carbon/Info.plist.in >widgets.app/Contents/Info.plist
	
	
	/bin/echo "APPL????" >widgets.app/Contents/PkgInfo
	
	
	ln -f widgets$(EXEEXT) widgets.app/Contents/MacOS/widgets
	
	
	cp -f $(top_srcdir)/src/osx/carbon/wxmac.icns widgets.app/Contents/Resources/wxmac.icns

